{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RangeSearchQuerySchema",
  "type": "object",
  "properties": {
    "rangerel": {
      "type": "string",
      "enum": [
        "before", "after", "lessThan", "greaterThan",
        "lessThanEquals", "greaterThanEquals",
        "during", "between"
      ]
    },
    "range": {
      "type": "integer"
    },
    "endRange": {
      "type": "integer"
    },
    "attribute": {
      "type": "string"
    }
  },
  "required": ["rangerel", "attribute"],
  "if": {
    "properties": {
      "rangerel": {
        "enum": ["during", "between"]
      }
    }
  },
  "then": {
    "required": ["range", "endRange"]
  },
  "else": {
    "required": ["range"]
  },
  "additionalProperties": true
}
